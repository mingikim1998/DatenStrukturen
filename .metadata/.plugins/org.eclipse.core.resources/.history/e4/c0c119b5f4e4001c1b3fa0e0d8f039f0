import java.util.Stack;
import java.util.ArrayList;
public class Main {
	static Stack<Character> stack = new Stack<Character>();
//	public class ListStack() {
//		private ArrayList stacks;
//	}
    public static void main(String[] args) {
    	Stack<Character> stack = new Stack<Character>();
    	stack.push('9');
    	stack.push('2');
    	stack.push('4');
    	stack.push('8');
    	stack.push('5');
    	stack.push('1');
    	System.out.println(sortStack());
    }
    public static char push(char value) {
    	return stack.push(value);
    }
    public boolean isEmpty(Stack<Character> stack) {
    	return stack.empty();
    }
    public static Stack sortStack() {
    	Stack<Character> tempStack = new Stack<Character>();
    	while (stack.empty()==false) {
    		char tmp = stack.pop();
    		while (tempStack.empty()== false && tempStack.peek()<tmp) {
    			stack.push(tempStack.pop());
    		}
    		tempStack.push(tmp);
    	}
    	return tempStack;
    }

}
    
