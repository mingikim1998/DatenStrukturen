import java.util.Stack;
import java.util.Scanner;
public class Main {
    public static void main(String[] args){
    	Scanner in = new Scanner(System.in);
    	Stack<Integer> stack = new Stack<>();
    	int a = in.nextInt();
    	for (int i=0; i<a; i++) {
    		String tmp = in.next();
    		if (tmp == "push") {stack.push(in.nextInt());}
    		else if (tmp == "pop"&&stack.empty() ==true) {System.out.println(stack.peek());stack.pop();}
    		//else if (tmp == "pop"&&stack.empty() ==false) {System.out.println(stack.peek());stack.pop();}
    		else if (tmp == "top"&&stack.empty()==true) {System.out.println(stack.peek());}
    		//else if (tmp == "top"&&stack.empty()==false) {System.out.println(-1);}
    		else if (tmp=="empty") {
    			if (stack.empty()==true) {System.out.println(1);}
    			if (stack.empty()==false) {System.out.println(0);}
    		}
    		else if (tmp == "size") {System.out.println(stack.size());}
    		else {System.out.println(-1);} // empty == false;
    	}
    	in.close();
	}
}
